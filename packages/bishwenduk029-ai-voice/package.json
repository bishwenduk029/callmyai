{
  "name": "@bishwenduk029/ai-voice",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "ui/dist/**/*",
    "server/dist/**/*"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --dts --external react",
    "dev": "tsup --format esm,cjs --watch --dts --external react",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@bishwenduk029/eslint-config": "workspace:*",
    "@bishwenduk029/tsconfig": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18.2.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^0.0.6",
    "@ricky0123/vad-react": "^0.0.22",
    "ai": "^3.1.14",
    "natural": "^7.0.5",
    "nltk": "^0.0.1",
    "node-emoji": "^2.1.3",
    "zod": "^3.23.8"
  },
  "exports": {
    "./ui": {
      "types": "./ui/dist/index.d.ts",
      "import": "./ui/dist/index.mjs",
      "require": "./ui/dist/index.js"
    },
    "./server": {
      "types": "./server/dist/index.d.ts",
      "import": "./server/dist/index.mjs",
      "require": "./server/dist/index.js"
    }
  }
}
